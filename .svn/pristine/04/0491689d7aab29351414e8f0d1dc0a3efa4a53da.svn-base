body {
	background:#eee
}
.search-crumbs {
	height:20px
}
.con-block {
	border:1px dashed #FF5C5C;
	color:#FF5C5C;
	padding:0 5px;
	height:18px;
	line-height:18px
}
.con-block i {
	cursor:pointer
}
.cate-wrap {
	width:1170px;
	font-size:14px
}
.cate-wrap li {
	padding:10px 0;
	margin:0 20px;
	/*border-bottom:1px solid #e1e1e1 */
}
.cate-wrap li.last {
	padding:10px 20px;
	margin:0
}
.cate-wrap .cate-label {
	width:60px;
	float:left;
	margin-top:6px
}
.cate-wrap .cate-more {
	width:60px;
	float:right;
	text-align:right;
	cursor:pointer;
	margin-top:8px;
	display:none
}
.cate-wrap .cate-block {
	margin:0 60px;
	height:31px;
	overflow:hidden;
	zoom:1
}
.cate-wrap .cate-block a {
	padding:0 18px 0 8px;
	cursor:pointer;
	height:31px;
	line-height:31px
}
.cate-wrap .cate-block a.active {
	background:#FF5C5C;
	color:#fff;
	padding:0 8px;
	margin-right:10px
}
.sort-label {
	width:60px;
	padding-top:5px
}
.sort-block {
	border:1px solid #e1e1e1;
	background:#f2f2f2;
	padding:5px 0;
	width:70px;
	text-align:center;
	margin-right:10px;
	cursor:pointer
}
.sort-block.active {
	background:#FF5C5C;
	color:#fff;
	border:1px solid #FF5C5C
}
.type-block {
	margin-top:5px;
	margin-right:15px
}
.type-block input {
	margin-top:3px;
	margin-right:5px
}
.goods-list li {
	width:220px;
	margin-right:15px;
	margin-bottom:20px
}
.goods-list li.tips {
	width:100%;
	margin-right:0;
	margin-bottom:0
}
.goods-list .title {
	height:40px;
	line-height:20px;
	overflow:hidden;
	zoom:1
}
.goods-list .btn {
	border-radius:0
}
.goods-list .no-margin {
	margin-right:0
}
.goods-list .goods-img {
	height:224px
}
.h-18 {
	height:18px;
	line-height:18px
}
.cheap-tag {
	background:url(../../images/cheap-tag.png) no-repeat;
	width:64px;
	height:61px;
	text-align:center;
	padding-top:5px;
	font-size:13px;
	line-height:18px
}
.profit-tag {
	background:url(../../images/profit-tag.png) no-repeat;
	width:52px;
	height:47px;
	padding-top:4px;
	text-align:center;
	font-size:13px;
	line-height:18px
}
.lh-150px {
	line-height:224px
}
.m-auto {
	margin:0 auto
}
.h-200px {
	height:200px;
	padding-top:80px
}
.banner-img {
	vertical-align:middle;
	display:inline-block;
	position:absolute;
	top:0;
	left:50%;
	margin-left:-960px
}
.h-350px {
	height:200px
}
.h-40px {
	height:40px;
	line-height:40px
}
.br-50 {
	-webkit-border-radius:50%;
	border-radius:50%
}
.new-icon-tips {
	top:-5px;
	right:10px;
	height:10px
}
.pr-5 {
	padding-right:5px
}
.h-15 {
	height:15px;
	line-height:15px
}
