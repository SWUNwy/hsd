.w-slider {
	height:450px;
	position:relative;
	overflow:hidden
}
.sliderContent {
	width:1190px;
	height:450px;
	margin:0 auto;
	position:relative
}
.slider-box {
	width:9999px;
	height:450px;
	position:absolute;
	top:0;
	left:0;
	margin-left:-365px;
	z-index:1
}
.slider-unit {
	width:1920px;
	float:left;
	text-align:center
}
.sliderTriggle {
	text-align:center;
	width:1190px;
	margin:0 auto;
	height:22px;
	position:relative;
	z-index:2;
	bottom:50px
}
.sliderTriggle .next, .sliderTriggle .prev {
	position:absolute;
	top:50%;
	margin-top:-225px;
	visibility:hidden;
	width:34px;
	height:68px;
transition:all .5s;
	opacity:.4;
	filter:alpha(opacity=40);
	cursor:pointer;
	background-color: #000;
	background-image: url(../images/seller/jt.png);
	background-repeat: no-repeat;
}
.w-slider:hover .next, .w-slider:hover .prev {
	visibility:visible
}
.sliderTriggle .prev {
	left:190px;
	background-position:9px 21px
}
.sliderTriggle .next {
	right:0;
	background-position:-25px 21px
}
.sliderCircle a {
	margin:0 1px;
	color:#675D71
}
.sliderCircle .a-num {
	top:12px;
	left:15px
}
.sliderCircle a.cur {
	color:#FF5C5C
}
div[aria-labelledby='title:popLayer'] .ui-dialog-body {
	padding:0
}
div[aria-labelledby='title:popLayer'] .ui-dialog {
	border:none;
	background:0 0;
	box-shadow:none
}
.down-app {
	position:fixed;
	bottom:0;
	left:0;
	right:0;
	z-index:110
}
.down-app-wrap {
	position:relative
}
.down-app-wrap .bg {
	background:#000;
	opacity:.7;
	-moz-opacity:.7;
	width:100%;
	height:120px
}
.down-img {
	position:absolute;
	width:1022px;
	margin-left:-511px;
	left:50%;
	bottom:0
}
.down-img img {
	width:1022px
}
.down-img a {
	position:absolute;
	right:30px;
	bottom:105px;
	color:#fff;
	background:#b61a1a;
	width:16px;
	height:16px;
	border-radius:50%;
	display:block;
	line-height:16px;
	text-align:center
}
